    "tscpaths": "^0.0.6",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.12.0",
    "tslint-eslint-rules": "^5.1.0",
    "tslint-jsdoc-rules": "^0.1.2",
    "tslint-microsoft-contrib": "^5.2.1",
    "tslint-react": "^3.5.1",
    "typedoc": "^0.13.0",
    "typescript": "^3.0.1",
    "utility-types": "^3.2.0",
