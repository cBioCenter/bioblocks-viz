{
  "compilerOptions": {
    "allowJs": false,
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "downlevelIteration": true,
    "jsx": "react",
    "lib": ["dom", "es2015", "es2016", "es2017"],
    "module": "commonjs",
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": false,
    "outDir": "./dist/",
    "paths": {
      "jest": ["node_modules/@types/jest/index.d.ts"],
      "plotly.js/dist/plotly": ["node_modules/@types/plotly.js"],
      "react": ["node_modules/@types/react"],
      "semantic-ui-react": ["node_modules/semantic-ui-react/index.d.ts"]
    },
    "sourceMap": true,
    "strict": true,
    "target": "es5",
    "typeRoots": ["./types/", "./node_modules/@types"]
  },
  "exclude": ["node_modules"],
  "include": ["src/**/*", "types", "configs/**/*", "__mocks__/**/*", "./chell-viz.ts"],
  "typeAcquisition": {
    "enable": false
  }
}
