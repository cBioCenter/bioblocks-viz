{
  "defaultSeverity": "error",
  "extends": [
    "tslint:latest",
    "tslint:recommended",
    "tslint-jsdoc-rules",
    "tslint-react",
    "tslint-eslint-rules",
    "tslint-config-prettier"
  ],
  "jsRules": {},
  "rules": {
    "jsdoc-format": [true],
    "no-console": false,
    "no-implicit-dependencies": [true, "dev", ["__mocks__", "configs"]],
    "no-namespace": [true, "allow-declarations"],
    "no-redundant-jsdoc": true,
    "no-reference": false,
    "no-submodule-imports": [true, "__mocks__", "configs", "plotly.js/dist/plotly"],
    "quotemark": [true, "jsx-double", "single", "avoid-escape"]
  },
  "rulesDirectory": []
}
